<view class='container'>
  <block wx:for="{{collList}}">
    <view class='coll-item'  data-id-Value="{{item.id}}" bindtap='collDetail'>
      <view>
        <image src='{{item.imgUrl}}'></image>
      </view>
      <view class='item-bottom-bar'>
        <text>{{item.title}}</text>
        <button catchtap='tapDelete' data-id-Value="{{item.id}}">删除收藏</button>
      </view>
    </view>
  </block>
</view>

