.container{
  background-color: #f5f5f5;
}
.coll-item{
  margin-bottom: 30rpx;
}
.coll-item image{
  width: 750rpx;
  height: 400rpx;
}
.item-bottom-bar{
  height: 50rpx;
  padding: 25rpx 0;
  background-color: #fff;
}
.item-bottom-bar text{
  float: left;
  font-size: 30rpx;
  line-height: 50rpx;
  text-indent: 40rpx;
}
.item-bottom-bar button{
  border-width: 0px;
  font-size: 26rpx;
  color: #666;
  float: right;
  margin-right: 40rpx;
  background-color: #fff;
  height: 50rpx;
  line-height: 50rpx;
}
